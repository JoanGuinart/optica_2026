# Controla si mostrar la página "en construcción"
# true = mostrar página en construcción
# false = mostrar sitio normal
PUBLIC_UNDER_CONSTRUCTION=false

# Control de tema forzado
# "auto" = automático (respeta preferencia del usuario)
# "light" = solo modo claro (fuerza siempre blanco)
# "dark" = solo modo oscuro (fuerza siempre negro)
PUBLIC_FORCE_THEME=light